import geshaDeep;

List[List[INT]] x_train = [
    [0],
    [1]
];
List[INT] y_train = [1, 0];   # NOT(0) = 1, NOT(1) = 0

List[List[INT]] x_val = [
    [0]
];
List[INT] y_val = [1];

List[List[INT]] x_test = [
    [1]
];
List[INT] y_test = [0];

GESHA model = geshaDeep.binary();
GESHA layer = geshaDeep.create_dense(1, "", [1], 0.0); 
model.add(layer);

model.compile("sgd", "binary_crossentropy", ["accuracy"]);

model.fit(x_train, y_train, 200, 1, x_val, y_val);

model.summary();
model.evaluate(x_test, y_test);
