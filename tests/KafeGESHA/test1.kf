import geshaDeep;

List[List[INT]] x_train = [
    [0, 0],
    [0, 1],
    [1, 0],
    [1, 1]
];
List[INT] y_train = [0, 0, 0, 1];

List[List[INT]] x_val = [
    [0, 1],
    [1, 1]
];
List[INT] y_val = [0, 1];

List[List[INT]] x_test = [
    [1, 0],
    [1, 1]
];
List[INT] y_test = [0, 1];

GESHA model = geshaDeep.binary();
GESHA layer = geshaDeep.create_dense(1, "identidad", [2], 0.0, 43);
model.add(layer);

model.compile("sgd", "binary_crossentropy", ["accuracy"]);

model.fit(x_train, y_train, 200, 1, x_val, y_val);

model.summary();
model.evaluate(x_test, y_test);
