import geshaDeep;

List[List[INT]] x_train = [
    [0, 0],
    [0, 1],
    [1, 0],
    [1, 1]
];
List[INT] y_train = [0, 1, 1, 0];  

List[List[INT]] x_val = [
    [0, 1],
    [1, 1]
];
List[INT] y_val = [1, 0];

List[List[INT]] x_test = [
    [1, 0],
    [1, 1]
];
List[INT] y_test = [1, 0];

GESHA model = geshaDeep.categorical();

GESHA hidden_layer = geshaDeep.create_dense(2, "relu", [2], 0.0, 42);
model.add(hidden_layer);


GESHA output_layer = geshaDeep.create_dense(1, "sigmoide", [2], 0.0, 42);
model.add(output_layer);

model.compile("sgd", "binary_crossentropy", ["accuracy"]);

model.fit(x_train, y_train, 1000, 1, x_val, y_val);

model.summary();
model.evaluate(x_test, y_test);
