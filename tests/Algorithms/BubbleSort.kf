drip bubbleSort(arr: List[INT]) => List[INT]:
    INT n = len(arr);
    INT i = 0;
    INT temp;
    INT j;
    while (i < n):
        j = 0;
        while (j < n - i - 1):
            if (arr[j] > arr[j + 1]):
                temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            ;
            j = j + 1;
        ;
        i = i + 1;
    ;
    return(arr);
;

List[INT] miLista = [5, 4324, 8, 4, 2];
List[INT] ordenada = bubbleSort(miLista);
show(ordenada);

