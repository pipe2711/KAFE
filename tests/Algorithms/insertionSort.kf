List[INT] lst = [3, 400, 32432, 46, 223, 9];
List[INT] sortedList = [];
INT i;
INT j;
INT key;


i = 0;
while (i < len(lst)):
    append(sortedList, lst[i]);
    i = i + 1;
;


i = 1;
while (i < len(sortedList)):
    key = sortedList[i];
    j = i - 1;

    while (j >= 0 and sortedList[j] > key):
        sortedList[j + 1] = sortedList[j];
        j = j - 1;
    ;

    sortedList[j + 1] = key;
    i = i + 1;
;

show("Lista ordenada:");
show(sortedList);
