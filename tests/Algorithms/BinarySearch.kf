drip binarySearch(lst: List[INT], target: INT) => INT:
    INT left = 0;
    INT right = len(lst) - 1;
    INT mid;

    while (left <= right):
        mid = int((left + right) / 2);

        if (lst[mid] == target):
            return mid;
        ;
        if (lst[mid] < target):
            left = mid + 1;
        else:
            right = mid - 1;
        ;
    ;

    return -1;
;


List[INT] data = [5, 10, 15, 20, 25, 30, 35];
INT index = binarySearch(data, 25);
show("Índice encontrado:");
show(index);
