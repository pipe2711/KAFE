drip linearSearch(lst: List[INT], target: INT) => INT:
    INT i = 0;
    while (i < len(lst)):
        if (lst[i] == target):
            return i;  
        ;
        i = i + 1;
    ;
    return -1;  
;


List[INT] data = [10, 20, 30, 40, 50];
INT result = linearSearch(data, 30);
show("Índice encontrado:");
show(result);
